        </a>
      </div>
      {{ page.headernav }}
      <button class="header__nav-toggle js-nav-toggle"></button>
    </div>
  </div>
</header>
  {{ page.breadcrumb }}

  {% if page.highlighted %}
    {{ page.highlighted }}
  {% endif %}

  {{ page.help }}

<main role="main">
  <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

  {% if page.sidebar %}
    <div class="container">
      <h1 class="mt-3 mt-md-5">Find campingplads</h1>
      <div class="search-links text-md-right"><a href="/campingpladser-med-kort">{{ 'Go to search with map filters'|t }}</a></div>
      <div class="row mt-3 mt-md-5">
        <div class="col-12 col-md-4">
          {{ page.sidebar }}
        </div>
        <div class="col-12 col-md-8">
          {{ page.content }}
        </div>
      </div>
    </div>
  {% else %}
    {{ page.content }}
  {% endif %}

</main>
<footer role="contentinfo">
  {{ page.footertop }}
  {{ page.footerbottom }}
</footer>
<div id="consentmodalcnt"></div>
